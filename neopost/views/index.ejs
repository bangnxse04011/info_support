  <!-- Start About us -->
  <section id="mu-gallery">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="mu-gallery-area">
            <div class="mu-title">
              <!-- <span class="mu-subtitle">Event</span> -->
              <h2>Chiến dịch</h2>
              <h5>Visharee kết nối người dùng với các nhãn hàng cùng tham gia vào các hoạt động cộng đồng.</h5>
            </div>
            <div class="mu-gallery-content">
              <!-- Start gallery image -->
              <div class="mu-gallery-body">
                <% event.forEach(function(info) { %>
                  <!-- start single gallery image -->
                  <div class="mu-single-gallery col-md-4">
                    <div class="mu-single-gallery-item">
                      <figure class="mu-single-gallery-img">
                        <a href="/events/<%= info.id %>">
                          <img alt="img" src="/stylesheets/images/<%=info.path_img %>">
                          <div class="mu-single-gallery-info"></div>
                        </a>
                      </figure>
                    </div>
                    <a href="/events/<%= info.id %>">
                      <div>
                        <h3 style="text-align: center;">
                          <b>
                            <%=info.fullName %>
                          </b>
                        </h3>
                        <h5 style="white-space: nowrap;
                        overflow: hidden;
                        text-overflow: ellipsis;">
                          <b>
                            <%=info.address %>
                          </b>
                          <br/>
                          <br/>
                        </h5>
                        <hr/>
                        <div style="text-align: center;" class="col-sm-6">
                          <p style="font-size: 20px">
                            <span id="number_ungho">
                              <%=info.total_view %>
                            </span>
                          </p>
                          Ủng hộ
                        </div>
                        <div style="text-align: center;" class="col-sm-6">
                          <p style="font-size: 20px">
                            <span id="number_muctieu">
                              <%=info.money %>
                            </span>

                          </p>
                          Mục tiêu
                        </div>
                        <button type="button" style="height:45px" class=" btn-lg btn-block btn btn-info">
                          <b>Hành Động</b>
                        </button>
                      </div>
                    </a>
                  </div>
                  <% }); %>
                    <!-- End single gallery image -->
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

